            x_min, x_max = <span class="hljs-built_in">min</span>(x_coords), <span class="hljs-built_in">max</span>(x_coords)
            y_min, y_max = <span class="hljs-built_in">min</span>(y_coords), <span class="hljs-built_in">max</span>(y_coords)
            
            bbox = [x_min, y_min, x_max, y_max]
            label = obj[<span class="hljs-string">'label'</span>]
