19.8372C9.72813 19.712 10.1037 19.6494 10.4542 19.521C10.7652 19.407 11.0608 19.2549 11.3343 19.068C11.6425 18.8575 11.9118 18.5882 12.4503 18.0497L20 10.5C21.3807 9.11929 21.3807 6.88071 20 5.5C18.6193 4.11929 16.3807 4.11929 15 5.5L7.45026 13.0497C6.91175 13.5882 6.6425 13.8575 6.43197 14.1657C6.24513 14.4392 6.09299 14.7348 5.97903 15.0458C5.85062 15.3963 5.78802 15.7719 5.66282 16.5231Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path><path d="M14.5 7L18.5 11" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path></svg>Edit</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre language-python"><span><span class="hljs-keyword">import</span> cv2
<span class="hljs-keyword">import</span> numpy <span class="hljs-keyword">as</span> np
<span class="hljs-keyword">import</span> matplotlib.pyplot <span class="hljs-keyword">as</span> plt

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 1️⃣ Image Acquisition</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Load an image from file</span>
img = cv2.imread(<span class="hljs-string">'image.jpg'</span>)
<span class="hljs-keyword">if</span> img <span class="hljs-keyword">is</span> <span class="hljs-literal">None</span>:
    <span class="hljs-keyword">raise</span> ValueError(<span class="hljs-string">"Image not found. Please check the file path."</span>)

<span class="hljs-comment"># Convert image from BGR (OpenCV default) to RGB for display</span>
img_rgb = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 2️⃣ Image Enhancement</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Enhance the image using histogram equalization on the luminance channel.</span>
<span class="hljs-comment"># We'll convert the image to YCrCb, equalize the Y channel, and convert back.</span>

img_ycrcb = cv2.cvtColor(img, cv2.COLOR_BGR2YCrCb)
y, cr, cb = cv2.split(img_ycrcb)
y_eq = cv2.equalizeHist(y)  <span class="hljs-comment"># Histogram equalization</span>
img_ycrcb_eq = cv2.merge([y_eq, cr, cb])
img_enhanced = cv2.cvtColor(img_ycrcb_eq, cv2.COLOR_YCrCb2BGR)
img_enhanced_rgb = cv2.cvtColor(img_enhanced, cv2.COLOR_BGR2RGB)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 3️⃣ Image Restoration</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Restore the image by reducing noise using Non-Local Means Denoising.</span>
img_restored = cv2.fastNlMeansDenoisingColored(img, <span class="hljs-literal">None</span>, h=<span class="hljs-number">10</span>, hColor=<span class="hljs-number">10</span>, templateWindowSize=<span class="hljs-number">7</span>, searchWindowSize=<span class="hljs-number">21</span>)
img_restored_rgb = cv2.cvtColor(img_restored, cv2.COLOR_BGR2RGB)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 4️⃣ Morphological Processing</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Convert to grayscale then apply an opening operation (erosion followed by dilation)</span>
<span class="hljs-comment"># to remove small noise.</span>
img_gray = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)
kernel = np.ones((<span class="hljs-number">5</span>, <span class="hljs-number">5</span>), np.uint8)
img_morph = cv2.morphologyEx(img_gray, cv2.MORPH_OPEN, kernel)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 5️⃣ Segmentation</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Use Otsu's thresholding to segment the image.</span>
ret, img_thresh = cv2.threshold(img_gray, <span class="hljs-number">0</span>, <span class="hljs-number">255</span>, cv2.THRESH_BINARY + cv2.THRESH_OTSU)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 6️⃣ Object Recognition</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># A simple example: find contours in the thresholded image and draw bounding boxes.</span>
contours, hierarchy = cv2.findContours(img_thresh, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)
img_contours = img.copy()
<span class="hljs-keyword">for</span> cnt <span class="hljs-keyword">in</span> contours:
    <span class="hljs-keyword">if</span> cv2.contourArea(cnt) &gt; <span class="hljs-number">100</span>:  <span class="hljs-comment"># Filter small contours</span>
        x, y, w, h = cv2.boundingRect(cnt)
        cv2.rectangle(img_contours, (x, y), (x + w, y + h), (<span class="hljs-number">0</span>, <span class="hljs-number">255</span>, <span class="hljs-number">0</span>), <span class="hljs-number">2</span>)
img_contours_rgb = cv2.cvtColor(img_contours, cv2.COLOR_BGR2RGB)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 7️⃣ Representation &amp; Description</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Use ORB (Oriented FAST and Rotated BRIEF) to detect keypoints and compute descriptors.</span>
orb = cv2.ORB_create()
keypoints, descriptors = orb.detectAndCompute(img_gray, <span class="hljs-literal">None</span>)
img_orb = cv2.drawKeypoints(img, keypoints, <span class="hljs-literal">None</span>, color=(<span class="hljs-number">0</span>, <span class="hljs-number">255</span>, <span class="hljs-number">0</span>), flags=<span class="hljs-number">0</span>)
img_orb_rgb = cv2.cvtColor(img_orb, cv2.COLOR_BGR2RGB)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 8️⃣ Image Compression</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Compress the image by encoding it into JPEG format with reduced quality.</span>
encode_param = [<span class="hljs-built_in">int</span>(cv2.IMWRITE_JPEG_QUALITY), <span class="hljs-number">50</span>]  <span class="hljs-comment"># Quality level 50 (on a scale from 0 to 100)</span>
result, encimg = cv2.imencode(<span class="hljs-string">'.jpg'</span>, img, encode_param)
<span class="hljs-keyword">if</span> <span class="hljs-keyword">not</span> result:
    <span class="hljs-keyword">raise</span> Exception(<span class="hljs-string">"Image encoding failed"</span>)
img_compressed = cv2.imdecode(encimg, <span class="hljs-number">1</span>)
img_compressed_rgb = cv2.cvtColor(img_compressed, cv2.COLOR_BGR2RGB)

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># 9️⃣ Colour Image Processing</span>
<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Convert the image to the HSV colour space and display one of its channels (e.g., Hue).</span>
img_hsv = cv2.cvtColor(img, cv2.COLOR_BGR2HSV)
h, s, v = cv2.split(img_hsv)  <span class="hljs-comment"># Split into channels</span>

<span class="hljs-comment"># --------------------------------------</span>
<span class="hljs-comment"># Plotting the results</span>
<span class="hljs-comment"># --------------------------------------</span>
fig, axs = plt.subplots(<span class="hljs-number">3</span>, <span class="hljs-number">3</span>, figsize=(<span class="hljs-number">16</span>, <span class="hljs-number">16</span>))

<span class="hljs-comment"># Original Image</span>
axs[<span class="hljs-number">0</span>, <span class="hljs-number">0</span>].imshow(img_rgb)
