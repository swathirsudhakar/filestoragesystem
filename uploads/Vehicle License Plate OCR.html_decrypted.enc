an></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre language-python"><span><span class="hljs-keyword">import</span> json

<span class="hljs-comment"># Load polygon annotation file</span>
<span class="hljs-keyword">with</span> <span class="hljs-built_in">open</span>(<span class="hljs-string">"path/to/your/annotations.json"</span>, <span class="hljs-string">"r"</span>) <span class="hljs-keyword">as</span> f:
    data = json.load(f)

<span class="hljs-keyword">for</span> annotation <span class="hljs-keyword">in</span> data[<span class="hljs-string">"annotations"</span>]:
    <span class="hljs-comment"># Get polygon points</span>
    polygon = annotation[<span class="hljs-string">"segmentation"</span>][<span class="hljs-number">0</span>]
    
    <span class="hljs-comment"># Compute bounding box (min/max of x and y)</span>
    x_coords = polygon[<span class="hljs-number">0</span>::<span class="hljs-number">2</span>]
    y_coords = polygon[<span class="hljs-number">1</span>::<span class="hljs-number">2</span>]
    
    x_min, x_max = <span class="hljs-built_in">min</span>(x_coords), <span class="hljs-built_in">max</span>(x_coords)
    y_min, y_max = <span class="hljs-built_in">min</span>(y_coords), <span class="hljs-built_in">max</span>(y_coords)
    
    <span class="hljs-comment"># Replace polygon with bounding box</span>
    annotation[<span class="hljs-string">"bbox"</span>] = [x_min, y_min, x_max - x_min, y_max - y_min]
    <span class="hljs-keyword">del</span> annotation[<span class="hljs-string">"segmentation"</span>]  <span class="hljs-comment"># Remove polygon data</span>

<span class="hljs-comment"># Save new annotation file</span>
<span class="hljs-keyword">with</span> <span class="hljs-built_in">open</span>(<span class="hljs-string">"path/to/your/bounding_box_annotations.json"</span>, <span class="hljs-string">"w"</span>) <span class="hljs-keyword">as</span> f:
    json.dump(data, f, indent=<span class="hljs-number">4</span>)

