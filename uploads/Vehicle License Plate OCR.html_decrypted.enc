            })
            annotation_id += <span class="hljs-number">1</span>

    coco_dict = {<span class="hljs-string">"images"</span>: images, <span class="hljs-string">"annotations"</span>: annotations, <span class="hljs-string">"categories"</span>: categories}
    
    <span class="hljs-keyword">with</span> <span class="hljs-built_in">open</span>(output_json, <span class="hljs-string">"w"</span>) <span class="hljs-keyword">as</span> json_file:
